{
    "name": "n8n-nodes-amorce",
    "version": "0.1.0",
    "description": "n8n nodes to connect with Amorce - the Universal Trust Protocol for AI Agents",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "amorce",
        "ai-agents",
        "langchain",
        "crewai",
        "agent-trust"
    ],
    "license": "MIT",
    "homepage": "https://amorce.io",
    "author": {
        "name": "Amorce",
        "email": "hello@amorce.io",
        "url": "https://amorce.io"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/AmorceAATP/n8n-nodes-amorce.git"
    },
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && gulp build:icons",
        "dev": "tsc --watch",
        "test": "vitest run",
        "test:watch": "vitest",
        "lint": "eslint nodes credentials --ext .ts",
        "lintfix": "eslint nodes credentials --ext .ts --fix",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/AmorceApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Amorce/AmorceAgent.node.js",
            "dist/nodes/Amorce/AmorceTrigger.node.js"
        ]
    },
    "devDependencies": {
        "@types/node": "^20.10.0",
        "eslint": "^8.57.0",
        "gulp": "^4.0.2",
        "n8n-workflow": "^1.20.0",
        "typescript": "^5.3.0",
        "vitest": "^1.0.0"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}